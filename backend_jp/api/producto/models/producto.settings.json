  "collectionName": "productos",
  "info": {
    "name": "Producto",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "disponibilidad": {
      "type": "boolean",
      "required": true
    },
    "precio": {
      "type": "integer",
      "required": true
    },
    "comentarios": {
      "type": "text",
      "required": false
    },
    "categoria": {
      "type": "string",
      "required": true
    },
    "escala": {
      "type": "string",
      "required": true
    },
    "imagen": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    }
  }
